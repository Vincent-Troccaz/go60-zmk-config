            wait-ms = <0>;
        };

        // Tréma : ë (¨ + e)

        e_diae: e_diae {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LS(LBKT) &kp E>;
            label = "ë";
            wait-ms = <0>;
