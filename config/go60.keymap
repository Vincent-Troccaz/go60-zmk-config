                <&kp KP_N1>,
                <&macro_tap>,
                <&kp KP_N1>,
                <&macro_tap>,
                <&kp ENTER>;

            label = "‑";
        };

        // — (em dash) U+2014

        em_dash: em_dash {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            wait-ms = <50>;
            tap-ms = <50>;
            bindings =
                <&macro_press>,
                <&kp LCTRL>,
                <&macro_press>,
                <&kp LSHIFT>,
                <&macro_tap>,
                <&kp U>,
                <&macro_release>,
                <&kp LSHIFT>,
