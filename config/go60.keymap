        u_circ: u_circ {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LBKT &kp U>;
            label = "û";
        };

        // Tréma : ï (¨ + i)

        i_diae: i_diae {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LS(LBKT) &kp I>;
            label = "ï";
            wait-ms = <0>;
        };

        // Tréma : ü (¨ + u)

        u_diae: u_diae {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LS(LBKT) &kp U>;
